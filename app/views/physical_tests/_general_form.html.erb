<div class='row justify-content-sm-center general-form'>
  <div class='col-sm-12 form-title'>
    <h5>Programa</h5>
  </div>
  <div class='col-sm-10 form-content'>
    <div class='form-group row justify-content-sm-center'>
      <%= f.select :suggested_program, suggested_program_options_for_select, { include_blank: 'Programa sugerido' }, class: 'col-sm-8 form-control', required: true %>
    </div>
    <div class='form-group row justify-content-sm-center'>
      <%= f.text_area :general_notes, placeholder: 'Notas generales', class: 'col-sm-8 form-control' %>
    </div>
  </div>
</div>
<br/>
