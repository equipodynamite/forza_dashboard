<div class='card'>
  <div class='card-body'>
    <h5 class='card-title'> Buscar miembro </h5>
    <%= form_tag(admin_dashboard_member_progress_path, method: 'get') do %>
      <div class='form-group row'>
        <div class='col-sm-10'>
          <%= text_field_tag(:username, value = "", class: "typeahead form-control", placeholder: "Nombre de usuario") %>
        </div>
        <div class='col-sm-2'>
          <%= submit_tag 'Buscar', :name => nil, class: 'btn btn-success btn-full-width' %>
        </div>
      </div>
    <% end %>
  </div>
</div>

